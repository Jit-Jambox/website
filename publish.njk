<!DOCTYPE html>
<html lang="en">

<head>
    {% include 'partials/header.njk' %}
    <link rel="stylesheet" href="/css/reset.css">
    <link rel="stylesheet" href="/css/publish.css">
    <link rel="stylesheet" href="/css/aos.css">
</head>

<body>
    <div class="body-inner">

       {% include 'partials/nav.njk' %}

        <div class="small-header">
            <div class="container">
                <div class="header-inner">
                    <h1 data-aos="fade-right"><span class="c-b">Level-Up</span> With <span class="c-o">Us</span></h1>
                </div>
            </div>
            <div id="particlesHeader">

            </div>
        </div>

        <section class="contact">
            <div class="container">
                <div class="contact-inner">
                    <div class="mt-2" id="mc_embed_signup">
                        <div id="mc_embed_signup">
                            <form action="https://games.us2.list-manage.com/subscribe/post?u=2f424a0817cc2284fdc987047&amp;id=2ada67a8de" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank">
                                <div id="mc_embed_signup_scroll">
                                    <div class="form-group">
                                        <div class="mc-field-group">
                                            <label class="s-l" for="mce-FNAME">First Name </label>
                                            <input required type="text" value="" name="FNAME" class="" id="mce-FNAME">
                                        </div>
                                        <div class="mc-field-group">
                                            <label class="s-l" for="mce-LNAME">Last Name </label>
                                            <input required type="text" value="" name="LNAME" class="" id="mce-LNAME">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="mc-field-group">
                                            <label class="s-l" for="mce-EMAIL">Email Address 
                                            </label>
                                            <input required type="email" value="" name="EMAIL" class="required email" id="mce-EMAIL">
                                        </div>
                                        <div class="mc-field-group size1of2">
                                            <label class="s-l" for="mce-PHONE">Phone Number </label>
                                            <input required type="text" name="PHONE" class="" value="" id="mce-PHONE">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="mc-field-group">
                                            <label class="s-l" for="mce-MMERGE6">App Store\Game play link </label>
                                            <input type="url" value="" name="MMERGE6" class=" url" id="mce-MMERGE6">
                                        </div>
                                    </div>
                                    <div id="mce-responses" class="clear">
                                        <div class="response" id="mce-error-response" style="display:none"></div>
                                        <div class="response" id="mce-success-response" style="display:none"></div>
                                    </div>
                                    <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
                                    <div style="position: absolute; left: -5000px;" aria-hidden="true"><input type="text" name="b_2f424a0817cc2284fdc987047_2ada67a8de" tabindex="-1" value=""></div>
                                    <div class="clear"><button type="submit" name="subscribe" id="mc-embedded-subscribe" class="btn s-r">Submit</button></div>
                                </div>
                            </form>
                        </div>

                        <!--End mc_embed_signup-->
                    </div>

                    <!--End mc_embed_signup-->
                </div>
            </div>
        </section>

{% include 'partials/footer.njk' %}

    <script>
        window.addEventListener('load', () => {
            let tag3 = document.createElement("script");
            tag3.src = "/js/publish.js";
            document.getElementsByTagName("head")[0].appendChild(tag3);
        })
    </script>

</body>

</html>